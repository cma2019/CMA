<!--pages/TrainingApplication/ModifyApplication/ModifyApplication.wxml-->
<view>
  <form bindsubmit='intercheckmodify'>
  <van-cell-group>
      <van-field 
        name="staffId"
        label="人员编号"
        value='{{staffId}}'
        readonly
        required
        clearable
      />
      <van-field 
        name="qualificationId"
        label="资质编号"
        value='{{qualificationId}}'
        placeholder="资质编号" 
    required
    readonly
        clearable
      />
      <van-field 
        name="qualificationName"
        label="资质名称"
        value='{{qualificationName}}'
        placeholder="资质名称" 
        required
        clearable
      />
    </van-cell-group>
    

    <view>
      <button class='van-button--info' formType='submit'>确定</button>
    </view>
  </form>
</view>
<van-button type="info" size="small" bindtap="goback">返回</van-button> 

