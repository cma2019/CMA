<!--pages/TrainingApplication/ModifyApplication/ModifyApplication.wxml-->
<view>
  <form bindsubmit='intercheckmodify'>
  <van-cell-group>
      <van-field 
        name="staffId"
        label="人员编号"
        value='{{staffId}}'
        readonly
        required
        clearable
      />
      <van-field 
        name="fileId"
        label="档案编号"
        value='{{fileId}}'
        placeholder="档案编号" 
        
    required
        clearable
      />
      <van-field 
        name="fileLocation"
        label="档案存放位置"
        value='{{fileLocation}}'
        placeholder="档案存放位置" 
        required
        clearable
      />
    </van-cell-group>
    

    <view>
      <button class='van-button--info' formType='submit'>确定</button>
    </view>
  </form>
</view>
<van-button type="info" size="small" bindtap="goback">返回</van-button> 

