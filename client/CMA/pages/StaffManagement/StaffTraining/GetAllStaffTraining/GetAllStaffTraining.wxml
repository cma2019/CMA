<!--pages/StaffManagement/StaffManagement.wxml-->
<view>
  <van-cell-group title="StaffTraining">
  <view wx:for="{{mess}}" wx:for-index="id" wx:for-item="item" wx:key="unique">
  <van-cell >
      <view slot="title">
        <view>trainingId : {{item.trainingId}}</view>
        <view>program : {{item.program}}</view>
        <view>id : {{item.id}}</view>
        <view>name : {{item.name}}</view>
        <view>trainingDate : {{item.trainingDate}}</view>
      </view>
  </van-cell>
</view>
  </van-cell-group>
</view>
    <van-button type="info" size="small" bindtap="gotoAdd">go back</van-button>
