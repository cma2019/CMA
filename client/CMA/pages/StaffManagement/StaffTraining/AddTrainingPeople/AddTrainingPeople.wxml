<!--pages/StaffManagement/AddStaff/AddStaff.wxml-->
<view>
  <form bindsubmit='ApplicationAdd'>
   <van-cell-group>
      <van-field 
        name="trainingId"
        label="培训编号"
        value="{{trainingId}}"
        readonly
        required
        clearable
      />
      <van-field 
        name="id"
        label="人员编号"
        placeholder="人员编号" 
        required
        clearable
      />
    </van-cell-group>
    <view>
      <button class='van-button--info'  formType='submit'>确定</button>
    </view>
  </form>
</view>
