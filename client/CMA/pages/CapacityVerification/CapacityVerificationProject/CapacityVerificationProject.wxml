<!--pages/IntermediateCheck/IntermediateCheck.wxml-->
<view>
  <form bindsubmit='GetProjects'>
    <van-cell-group title="请输入信息">
      <van-field 
        name="planId"
        label="项目序号"
        placeholder="项目序号"
        type='number'
        required
        clearable
      />
    </van-cell-group>
    <button class='van-button--info' formType='submit'>确认</button>
  </form>
</view>
<van-button type="info" size="small" bindtap="gotomenu">返回</van-button>