<!--pages/TrainingApplication/TrainingApplication.wxml-->
<view>
  <van-cell-group title="TrainingApplication">
  <view wx:for="{{mess}}" wx:for-index="id" wx:for-item="item" wx:key="unique">
  <van-cell value="详情" bindtap='gotoOne' id='{{item.id}}' is-link>
      <view slot="title">
        <view>name : {{item.name}}</view>
        <view>people : {{item.people}}</view>
        <view>trainingUnit : {{item.trainingUnit}}</view>
      </view>
  </van-cell>
</view>
  </van-cell-group>
</view>
    <van-button type="info" size="small" bindtap="gotoAdd">Add TrainingApplication</van-button> 