<!--pages/ExternalReviewManagement/ExternalReviewManagement/ExternalReviewManagementYear/ExternalReviewManagementYearGetOne/ExternalReviewManagementYearGetOne.wxml-->
<scroll-view scroll-y>
  <van-cell>
    <view slot="title">
      <view>id : {{id}}</view>
      <view>year : {{year}}</view>
      <view>fileId : {{fileId}}</view>
      <view>fileName : {{fileName}}</view>
    </view>
  </van-cell>
  <van-button type="info" size="small" bindtap="viewDetail">Modify</van-button>
  <van-button type="info" size="small" bindtap="mydelete">Delete</van-button>
  <van-button type="info" size="small" bindtap="mydownload">Download</van-button>
</scroll-view>
