<!--pages/Equipment/EquipmentMaintenance/EquipmentMaintenanceGetOne/EquipmentMaintenanceGetOne.wxml-->
<scroll-view scroll-y>
  <van-cell>
    <view slot="title">
      <view>id : {{equipment.id}}</view>
      <view>机身编号 : {{equipment.equipmentId}}</view>
      <view>记录日期 : {{equipment.maintenanceDate}}</view>
      <view>维护保养内容 : {{equipment.maintenanceContent}}</view>
      <view>维护保养人 : {{equipment.maintenancePerson}}</view>
      <view>确认人 : {{equipment.confirmer}}</view>
    </view>
  </van-cell>
  <van-button type="info" size="small" bindtap="viewDetail">修改</van-button>
  <van-button type="info" size="small" bindtap="mydelete">删除</van-button>
  <van-button type="info" size="small" bindtap="getAllById">此设备维护保养记录</van-button>
  <van-button type="info" size="small" bindtap="mygo">返回</van-button>
</scroll-view>

