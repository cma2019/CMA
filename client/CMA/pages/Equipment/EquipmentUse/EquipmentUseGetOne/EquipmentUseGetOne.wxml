<!--pages/Equipment/EquipmentUse/EquipmentUseGetOne/EquipmentUseGetOne.wxml-->
<scroll-view scroll-y>
  <van-cell>
    <view slot="title">
      <view>id : {{equipment.id}}</view>
      <view>equipmentId : {{equipment.equipmentId}}</view>
      <view>useDate : {{equipment.useDate}}</view>
      <view>openDate : {{equipment.openDate}}</view>
      <view>closeDate : {{equipment.closeDate}}</view>
      <view>sampleNumber : {{equipment.sampleNumber}}</view>
      <view>testProject : {{equipment.testProject}}</view>
      <view>beforeUse : {{equipment.beforeUse}}</view>
      <view>afterUse : {{equipment.afterUse}}</view>
      <view>user : {{equipment.user}}</view>
      <view>remark : {{equipment.remark}}</view>
    </view>
  </van-cell>
  <van-button type="info" size="small" bindtap="viewDetail">Modify</van-button>
  <van-button type="info" size="small" bindtap="mydelete">Delete</van-button>
</scroll-view>
