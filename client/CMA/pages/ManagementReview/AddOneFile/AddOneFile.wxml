<view>
  <form bindsubmit='ApplicationAdd'>
  <van-cell-group>
      <van-field 
        name="year"
        label="year"
        value='{{year}}'
        readonly
        required
        clearable
      />
      <van-field 
        name="fileName"
        label="fileName"
        placeholder="input fileName" 
        required
        clearable
      />
     
    </van-cell-group>
    <view>
      <button class='van-button--info'formType = 'submit'>Add           </button>
    </view>
  </form>
</view>
 <van-button type="info" size="small" bindtap="goback">go back</van-button> 

