<!--pages/IntermediateCheck/IntermediateCheckGetone/IntermediateCheckGetone.wxml-->
<view>
<van-cell-group title='message of {{planId}}'>
    <van-cell value="{{object}}"title="object"border="{{ false }}"/>
    <van-cell value="{{content}}"title="content"border="{{ false }}"/>
    <van-cell value="{{checkDate}}"title="checkDate"border="{{ false }}"/>
    <van-cell value="{{personInCharge}}"title="personInCharge"border="{{ false }}"/>
    <van-cell value="{{state}}"title="state"/>
</van-cell-group>
</view>
<button class='van-button--info' bindtap="modifyData">modify</button>
<button class='van-button--info' bindtap="deleteData">delete</button>
