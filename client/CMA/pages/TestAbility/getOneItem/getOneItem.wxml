<!--pages/TestAbility/getOneItem/getOneItem.wxml-->
<view>
<van-cell-group title='message of {{year}}'>
    <van-cell value="{{year}}"title="year"border="{{ false }}"/>
    <van-cell value="{{id}}"title="id"border="{{ false }}"/>
    <van-cell value="{{productionName}}"title="productionName"border="{{ false }}"/>
    <van-cell value="{{ability}}"title="ability"border="{{ false }}"/>
    <van-cell value="{{referrence}}"title="referrence"border="{{ false }}"/>
</van-cell-group>
</view>
<button class='van-button--info'bindtap="deleteData">delete</button>
