<!--pages/IntermediateCheck/IntermediateCheck.wxml-->
<view>
  <van-cell-group title="IntermediateCheck">
  <view wx:for="{{temp}}" wx:for-index="id" wx:for-item="item" wx:key="unique">
    <van-cell value="详情" bindtap='gotoOne' id='{{item.id}}' is-link>
      <view slot="title">
        <view>year : {{item.year}}</view>
        <view>id : {{item.id}}</view>
        <view>productionName : {{item.productionName}}</view>
      </view>
    </van-cell>
  </view>
  </van-cell-group>
</view>
<van-button type="info" size="small" bindtap="gotoAdd">Add an test ability</van-button>
<van-button type="info" size="small" bindtap="gotoModify">Modify year message</van-button>