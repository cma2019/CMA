<!--pages/SampleManagement/SampleIo/ioReceiptDetail/ioReceiptDetail.wxml-->
<view>
 <view wx:if="{{flag == 0}}">
    <van-cell-group>
      <van-field  label="sampleName" value='{{mess.sampleName}}' disabled input-align="right"/>
      <van-field  label="applicationUnit" value='{{mess.applicationUnit}}' disabled input-align="right"/>
      <van-field  label="version" value='{{mess.version}}' disabled input-align="right"/>
      <van-field  label="contractId" value='{{mess.contractId}}' disabled input-align="right"/>
      <van-field  label="testType" value='{{mess.testType}}' disabled input-align="right"/>
      <van-field  label="electronicMedia" value='{{mess.electronicMedia}}' disabled input-align="right"/>
      <van-cell-group>
        <view wx:for="{{mess.materialList}}"wx:key="{{index}}">
          <van-cell title="{{array[mess.materialList[index].materialId]}}" >
            <text class="cart-count-num">{{mess.materialType}}</text>
            <view wx:if="{{mess.materialName != null}}">
              <text>{{mess.materialName}}</text>
            </view>
          </van-cell>
        </view>
      </van-cell-group>
      <van-field  label="softwareType" value='{{mess.softwareType}}' disabled input-align="right"/>
      <van-field  label="receiveUnit" value='{{mess.receiveUnit}}' disabled input-align="right"/>
      <van-field  label="receiveDate" value='{{mess.receiveDate}}' disabled input-align="right"/>
      <van-field  label="sender" value='{{mess.sender}}' disabled input-align="right"/>
      <van-field  label="receiver" value='{{mess.receiver}}' disabled input-align="right"/>
    </van-cell-group>
  </view>
</view>
