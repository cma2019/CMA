<view>
 <form bindsubmit='SupervisionRecord_addone'>
    <van-cell-group title="SupervisionRecord_addone">
       <van-field v-model="department" name="department" required clearable label="部门" placeholder="请输入部门" />
      <van-field v-model="supervisor" name="supervisor" required clearable label="监督员" placeholder="请输入监督员" />
      <picker name = 'superviseDate' mode='date' value='{{superviseDate}}' bindchange='bindsuperviseDateChange'>
        <view class='picker'>
